{
    "kind":"BlueDeployment",
    "apiVersion":"apps/v1",
    "metadata":{
       "name":"blue",
       "labels":{
          "app":"blue"
       }
    },
    "spec":{
       "replicas":1,
       "selector":{
         "matchLabels":{
             "app":"blue"
         }
      },
       "template":{
          "metadata":{
             "labels":{
                "app":"blue"
             }
          },
          "spec":{
             "containers":[
                {
                   "name":"blue",
                   "image":"prakhyavanaparthy/capstoneproject",
                   "ports":[
                      {
                         "name":"blue-server",
                         "containerPort":80
                      }
                   ]
                }
             ]
          }
       }
    }
 }
 